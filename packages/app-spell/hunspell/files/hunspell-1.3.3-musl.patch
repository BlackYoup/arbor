From 8e85f9dfc595cabe6bda0d483c498c4eb646701e Mon Sep 17 00:00:00 2001
From: Felix Janda <felix.janda@posteo.de>
Date: Sat, 4 Apr 2015 08:15:14 +0200
Subject: [PATCH] [un]munch: Remove unnecessary includes

---
 configure.ac        | 2 +-
 src/tools/munch.c   | 5 -----
 src/tools/unmunch.c | 5 -----
 3 files changed, 1 insertion(+), 11 deletions(-)

diff --git a/configure.ac b/configure.ac
index 2e96619..d511966 100644
--- a/configure.ac
+++ b/configure.ac
@@ -28,7 +28,7 @@ AC_LIBTOOL_WIN32_DLL
 
 # Checks for header files.
 
-AC_CHECK_HEADERS([fcntl.h libintl.h locale.h unistd.h error.h])
+AC_CHECK_HEADERS([fcntl.h libintl.h locale.h unistd.h])
 
 # Checks for typedefs, structures, and compiler characteristics.
 AC_C_CONST
diff --git a/src/tools/munch.c b/src/tools/munch.c
index 52e7c51..e9c01b6 100644
--- a/src/tools/munch.c
+++ b/src/tools/munch.c
@@ -10,11 +10,6 @@
 #include <sys/stat.h>
 #include <fcntl.h>
 #include <limits.h>
-#if defined(__linux__) && !defined(__ANDROID__)
-#include <error.h>
-#include <errno.h>
-#include <sys/mman.h>
-#endif
 
 #include "munch.h"
 
diff --git a/src/tools/unmunch.c b/src/tools/unmunch.c
index c1d07a8..4c49220 100644
--- a/src/tools/unmunch.c
+++ b/src/tools/unmunch.c
@@ -11,11 +11,6 @@
 #include <sys/types.h>
 #include <sys/stat.h>
 #include <fcntl.h>
-#if defined(__linux__) && !defined(__ANDROID__)
-#include <error.h>
-#include <errno.h>
-#include <sys/mman.h>
-#endif
 
 #include "unmunch.h"
 
