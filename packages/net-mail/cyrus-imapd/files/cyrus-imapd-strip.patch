From 688cd0fd5b2a487a2b5aca2ba15f72c1d602d5c1 Mon Sep 17 00:00:00 2001
From: Wulf C. Krueger <philantrop@exherbo.org>
Date: Thu, 2 Apr 2009 22:05:11 +0200
Subject: [PATCH] Reason: Don't strip the binaries.
 Upstream: Wants it as-is.

---
 imtest/Makefile.in       |    2 +-
 installsieve/Makefile.in |    2 +-
 netnews/Makefile.in      |    2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/imtest/Makefile.in b/imtest/Makefile.in
index c73c39a..41e12f4 100644
--- a/imtest/Makefile.in
+++ b/imtest/Makefile.in
@@ -73,7 +73,7 @@ cyrus_prefix = @cyrus_prefix@
 all: imtest
 
 install:
-	$(INSTALL) -s -m 755 imtest $(DESTDIR)$(exec_prefix)/bin
+	$(INSTALL) -m 755 imtest $(DESTDIR)$(exec_prefix)/bin
 	ln -f $(DESTDIR)$(exec_prefix)/bin/imtest $(DESTDIR)$(exec_prefix)/bin/pop3test
 	ln -f $(DESTDIR)$(exec_prefix)/bin/imtest $(DESTDIR)$(exec_prefix)/bin/nntptest
 	ln -f $(DESTDIR)$(exec_prefix)/bin/imtest $(DESTDIR)$(exec_prefix)/bin/lmtptest
diff --git a/installsieve/Makefile.in b/installsieve/Makefile.in
index 7ace91d..5b27f7a 100644
--- a/installsieve/Makefile.in
+++ b/installsieve/Makefile.in
@@ -68,7 +68,7 @@ cyrus_prefix = @cyrus_prefix@
 all: installsieve
 
 install:
-	$(INSTALL) -s -m 755 installsieve $(DESTDIR)$(exec_prefix)/bin
+	$(INSTALL) -m 755 installsieve $(DESTDIR)$(exec_prefix)/bin
 
 OBJS=lex.o mystring.o request.o isieve.o
 
diff --git a/netnews/Makefile.in b/netnews/Makefile.in
index 897d370..19547c9 100644
--- a/netnews/Makefile.in
+++ b/netnews/Makefile.in
@@ -71,7 +71,7 @@ OBJS = remotepurge.o readconfig.o
 all: remotepurge
 
 install:
-	$(INSTALL) -s -m 755 remotepurge $(DESTDIR)$(service_path)
+	$(INSTALL) -m 755 remotepurge $(DESTDIR)$(service_path)
 
 .c.o:
 	$(CC) -c $(CPPFLAGS) $(DEFS) $(CFLAGS) $<
-- 
1.6.2.1

