[Unit]
Description=Miscellaneous basic configurations
DefaultDependencies=no
Conflicts=shutdown.target
Before=shutdown.target emergency.service emergency.target udev.service systemd-logger.socket
RefuseManualStart=yes
RefuseManualStop=yes

[Service]
Type=oneshot
StandardInput=tty
RemainAfterExit=yes
ExecStart=/bin/dmesg -n 1

[Install]
WantedBy=basic.target
